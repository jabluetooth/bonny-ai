{
  "buildCommand": "npm run build",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.filheinzrelatorre.com"
        }
      ],
      "destination": "https://filheinzrelatorre.com/:path*",
      "permanent": true
    }
  ]
}
